- @section = 'teams'

#yui-main
  .yui-b.box
    %h3 New Team

    = error_messages_for :team

    - form_for @team, :html => { :multipart => true, :class => 'MainForm' } do |f|
      = render :partial => 'form', :object => f
      %p= submit_tag :create.l, :or => link_to(:cancel.l, teams_path)

.yui-b
  .box.alt.first_alt
    = link_to 'Return to teams', teams_path
